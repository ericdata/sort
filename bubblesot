/*
 *bubble sort 
 */
void bubblesort(int *array, int length)
{
    int i, j, temp;
    for (i = 0; i < length; i++){
        for (j = 1; j < length - i; j++){
            if (array[j - 1] > array[j]){
                temp = array[j];
                array[j] = array[j-1];
                array[j-1] = temp;
            }
        }
    }
}
//end bubblesort
